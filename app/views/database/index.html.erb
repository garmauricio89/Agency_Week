<h1>Database</h1>

<table id="database table">
  <thead>
    <tr>
      <h2>Users Information</h2>    
        <th>First Name</th>
        <th>Last Name</th>
        <th>Username</th>
        <th>Email</th>
    </tr>
  </thead> 
        
  <tbody> 
    <% @users.each do |f| %>
      <tr>
        <td><%= f.fname %></td>
        <td><%= f.lname %></td>
        <td><%= f.username %></td>
        <td><%= f.email %></td>
      </tr> 
    <% end %>
  </tbody> 
</table>
  
  <br>