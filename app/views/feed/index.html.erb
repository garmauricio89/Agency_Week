    <div id="map-container">
    <div id="map-canvas"></div>
  </div>
  <div id="map_2">
  </div>
  <div id="users_post">
  <% @homelesspeople.each do |sighting| %>
    <h4><%= sighting.created_at.localtime.strftime("%a - %D "" %l: %M %p") %></h4>
    <h2><%= sighting.user.fname %></h2>
    <h2><%= sighting.user.email %></h2>
    <h3><%= sighting.lat %></h3>
    <h3><%= sighting.long %></h3>
  <% sighting.items.each do |i| %>
    <div id = "item"><strong><%= i.item_name %></strong></div>
    <% end %>
    <% end %>
  </div>



